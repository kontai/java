+
+public class test {
+    public static void main(String[] args) {
+        test t = new test();
+        System.out.println(t.rec(100));
+        System.out.println(t.regular(100));
+        System.out.println(t.j1(5));
+    }
+
+    public int rec(int i) {
+        if (i == 1) {
+            return 1;
+        } else {
+            return rec(i - 1) + i;
+        }
+    }
+
+    public int regular(int i) {
+        int sum = 0;
+        for (int j = 1; j <= i; j++) {
+            sum += j;
